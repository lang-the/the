obj Node {
  Tree? left
  Tree? right
  int val
}

class Tree {
  mut Node[] _nodes

  constructor (Node... nodes) {
    this._nodes = nodes
  }

  mut add (Node node) Tree {
    this._nodes.push(node)
    return this
  }
}

main {
  node1 := Node{ val: 1 }
  node2 := Node{ val: 2 }
  node3 := Node{ left: node1, right: node2, val: 3 }
  node4 := Node{ val: 4 }
  node5 := Node{ val: 5 }

  mut tree := new Tree(node3, node4)

  tree.add(node5)
  tree.add({ val: 6 })
}
